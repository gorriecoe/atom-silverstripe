'.text.html.ss':
  'editor':
    'commentStart': '<%-- '
    'commentEnd': ' --%>'
    'increaseIndentPattern': '(<% (control|loop|with|if|else_if) ([-_\\.\\(\\)\\& A-Za-z0-9]+) %>|<% else %>|<%--|<(?!area|base|br|col|frame|hr|html|img|input|link|meta|param)([-_\.\s\t\n\r\{\}\$\(\)\=\"\\\' A-Za-z0-9]+)(>|!\/>)(?!.*</\\1>)|<!--)'
    'decreaseIndentPattern': '(<% end_(control|loop|with|if) %>|<% else %>|<\/(?!html)([-_\\.\(\)A-Za-z0-9]+)>|-->)'
