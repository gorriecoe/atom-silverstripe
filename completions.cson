'Class Page & Controller Silverstripe':
  'prefix': 'classpageandcontroller'
  'module': 'silverstripe/cms'
  'minVersion': '3.0'
  'maxVersion': '4.0'
  'scope': '.text.html.php'
  'body': """
  /**
   * ${3:Description}
   *
   * @package silverstripe
   * @subpackage ${4:mysite}
   */
  class $1 extends ${2:Page}
  {
  \t${0}
  }
  class ${1}_Controller extends ${2:Page}_Controller
  {
  \tpublic function init()
  \t{
  \t\tparent::init();
  \t}
  }
  """

